<div class="author">
    <h1 class="author__heading">I am Temitope</h1>
    <p class="author__body">Web developer and content creatort</p>
</div>

<img src="https://s7d9.scene7.com/is/image/gobrightline/teacher-discount-hero-image:hero-mobile?fmt=webp-alpha&defaultImage=gobrightline/teacher-discount-hero-image&t=1703783923238" alt="temitope"  class="hero-image" width="981" height="528">
<section class="articles">
    <h2 class="articles__heading"> Latest posts </h2>

    
        <ul class="article-ul">
            <% data.forEach(post => { %>
           
            <li>
                <a href="/post/<%= post._id %>">
                    <span><%=  post.title %></span> 
                    <span class="article-list__date"><%= post.createdAt.toDateString()%></span>
                </a>
            </li>
            <% }) %>
        </ul>

        <% if (nextPage !== null) { %>
         <a href="/?page=<%= nextPage %>" class="pagination">&lt; View older posts</a>
        <% } %>

    
</section>

